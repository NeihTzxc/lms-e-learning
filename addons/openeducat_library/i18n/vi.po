#. module: openeducat_library
#: model:mail.template,body_html:openeducat_library.email_template_media_movement_after
msgid "\n"
"            <p>Dear <strong> ${object.student_id.name} </strong>\n"
"            <p>Your ${object.media_type_id.name}, <strong> ${object.media_id.name} </strong> has been expired. Please return it as soon as possible.</p>\n"
"            <p>Thank you.</p>\n"
"            "
msgstr "\n"
"            <P> Thưa <strong> $ {object.student_id.name} </ strong>\n"
"            <P> $ của bạn {} object.media_type_id.name, <strong> $ {object.media_id.name} </ strong> đã hết hạn. Vui lòng gửi lại nó càng sớm càng tốt. </ P>\n"
"            <P> Cảm ơn bạn. </ P>\n"
"            "

#. module: openeducat_library
#: model:mail.template,body_html:openeducat_library.email_template_media_movement
msgid "\n"
"            <p>Dear <strong> ${object.student_id.name} </strong>\n"
"            <p>Your ${object.media_type_id.name}, <strong> ${object.media_id.name} </strong> will \n"
"                % if object.get_diff_day() == 0:\n"
"                    be expiring <strong>today.</strong>\n"
"                % else:\n"
"                    expire in <strong> ${object.get_diff_day()}</strong> day(s).\n"
"                % endif\n"
"                Please return it as soon as possible.</p>\n"
"            <p>Thank you.</p>\n"
"            "
msgstr "\n"
"            <P> Thưa <strong> $ {object.student_id.name} </ strong>\n"
"            <P> $ của bạn {} object.media_type_id.name, <strong> $ {object.media_id.name} </ strong> sẽ\n"
"                % Nếu object.get_diff_day () == 0:\n"
"                    được hết hạn <strong> ngày hôm nay. </ strong>\n"
"                % Else:\n"
"                    hết hạn trong <strong> $ {object.get_diff_day ()} </ strong> day (s).\n"
"                % endif\n"
"                Vui lòng gửi lại nó càng sớm càng tốt. </ P>\n"
"            <P> Cảm ơn bạn. </ P>\n"
"            "

